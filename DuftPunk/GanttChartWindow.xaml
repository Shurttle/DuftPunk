<Window x:Class="DuftPunk.GanttChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DuftPunk"
        mc:Ignorable="d"
        Title="GanttChartWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <TextBlock Text="Gantt Chart" FontWeight="Bold" FontSize="16" Margin="10"/>

    
        <StackPanel Grid.Row="1" Margin="10">
            <Rectangle Fill="LightBlue" Width="100" Height="50" Margin="5"/>
            <Rectangle Fill="LightGreen" Width="200" Height="50" Margin="5"/>
           
        </StackPanel>
    </Grid>
</Window>
